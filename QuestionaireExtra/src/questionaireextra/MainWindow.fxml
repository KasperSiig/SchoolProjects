<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="347.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="questionaireextra.MainWindowController">
    <children>
        <Button fx:id="submit" layoutX="14.0" layoutY="122.0" onAction="#submitButton" text="Fill questionaire">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding></Button>
      <Label layoutX="14.0" layoutY="74.0" text="Name" />
      <TextField fx:id="txtName" layoutX="14.0" layoutY="91.0" />
      <ListView fx:id="listNames" layoutX="165.0" layoutY="40.0" onMouseClicked="#onNameClicked" prefHeight="127.0" prefWidth="172.0" />
      <Label layoutX="165.0" layoutY="6.0" text="Participants:" />
      <Button fx:id="btnGraph" layoutX="14.0" layoutY="154.0" mnemonicParsing="false" onAction="#showGraph" text="Graph for points" />
      <Label layoutX="165.0" layoutY="23.0" text="(double-click on name for more)" />
      <Button layoutX="165.0" layoutY="167.0" mnemonicParsing="false" onAction="#saveData" text="Save" />
      <Button layoutX="210.0" layoutY="167.0" mnemonicParsing="false" onAction="#delData" text="Clear" />
    </children>
</AnchorPane>
