<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="655.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="questionaireextra.QuestionWindowController">
   <children>
      <Label layoutX="10.0" layoutY="6.0" text="Name of the participant: ">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="lblName" layoutX="407.0" layoutY="6.0" text="Name">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="btnCalc" layoutX="14.0" layoutY="59.0" mnemonicParsing="false" onAction="#btnCalc" text="Calculate Score" />
      <Label layoutX="121.0" layoutY="54.0" text="Score: ">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="lblScore" layoutX="218.0" layoutY="54.0" text="0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ScrollPane layoutX="9.0" layoutY="99.0" prefHeight="446.0" prefWidth="638.0">
         <content>
            <AnchorPane prefHeight="443.0" prefWidth="622.0">
               <children>
                  <VBox fx:id="vboxQ" layoutX="10.0" layoutY="15.0" prefHeight="200.0" prefWidth="250.0" spacing="10.0" />
                  <VBox fx:id="vboxDisagree" layoutX="290.0" layoutY="15.0" prefHeight="200.0" prefWidth="100.0" spacing="10.0" />
                  <VBox fx:id="vboxNeutral" layoutX="390.0" layoutY="15.0" prefHeight="200.0" prefWidth="100.0" spacing="10.0" />
                  <VBox fx:id="vboxAgree" layoutX="490.0" layoutY="15.0" prefHeight="200.0" prefWidth="100.0" spacing="10.0" />
               </children>
            </AnchorPane>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
